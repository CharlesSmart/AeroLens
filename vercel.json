{
  "version": 2,
  "functions": {
    "api/signaling.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/signaling",
      "dest": "/api/signaling.ts",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/signaling/(.*)",
      "dest": "/api/signaling.ts",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
} 